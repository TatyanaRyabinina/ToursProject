<!DOCTYPE html>
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
	<meta charset="utf-8" />
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<title>@ViewBag.Title – приложение ASP.NET</title>
	@Styles.Render("~/Content/css")
	@Scripts.Render("~/bundles/modernizr")
	<link rel="stylesheet" type="text/css" media="screen" href="~/Content/ui.jqgrid.css" />
	<link rel="stylesheet" type="text/css" href="~/Content/jquery-ui.css" />
	<script src="~/Scripts/jQuery/jquery-1.10.2.min.js"></script>
	<script src="~/Scripts/jQuery/bootstrap.js"></script>
	<script src="~/Scripts/jQuery/jQuery.serializeObject.js"></script>
	<script src="~/Scripts/tourproject.js"></script>
	<script src="~/Scripts/tourproject.init.js"></script>
	<script src="~/Scripts/tourproject.initFunction.js"></script>
	<script src="~/Scripts/tourproject.login.js"></script>
	<script src="~/Scripts/tourproject.validate.js"></script>
	<script src="~/Scripts/tourproject.ajax.js"></script>
	<script src="~/Scripts/tourproject.tourPage.js"></script>
	<script src="~/Scripts/jQuerygrid/i18n/grid.locale-en.js" type="text/javascript"></script>
	<script src="~/Scripts/jQuerygrid/jquery.jqGrid.min.js" type="text/javascript"></script>
	<script src="~/Scripts/jQuery/jquery-ui.js" type="text/javascript"></script>
</head>
<body>
	@RenderBody()
	@RenderSection("scripts", required: false)
</body>
</html>
